{
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/${input:programPath}"
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "programPath",
      "description": "Select the Python file to debug",
      "options": ["main.py", "src/config_manager.py", "src/download_manager.py"]
    }
  ]
}
